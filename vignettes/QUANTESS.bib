@article{gemenis+:2009,
	Abstract = {Recent research has shown that, in several countries, the Com- 
parative Manifesto Pro ject (CMP) measures of party positions do not seem 
to perform well in terms of face validity and reliability. A fairly typical ex- 
ample of such a deviant case is that of Greece, where for the most part the 
findings based on this approach seem to suggest that parties' positions are 
characterised by extreme discontinuity and leapfrogging. Employing a differ- 
ent coding methodology whose departure point is that party competition is 
still a matter of direct confrontation between parties, this analysis attempts 
to measure the positions of Greek parties on three issue dimensions: level of 
state intervention in the economy, support toward the political integration of 
the EU, and common European cultural identity. According to its findings, 
the traditional left-right distinction is still evident in parties' economic stances 
whereas the other two issue dimensions indicate that there is an emerging new- 
politics dimension which distinguishes between the centripetal political forces 
on the one hand and the radical right and left parties on the other. Impor- 
tantly, this relatively new coding procedure seems to provide estimates which 
outperform those stemming from the CMP data both in terms of reliability 
and face validity.},
	Author = {K. Gemenis and E. Dinas},
	Date-Added = {2014-06-03 09:57:23 +0000},
	Date-Modified = {2014-06-03 09:57:23 +0000},
	Journal = {Comparative European Politics},
	Title = {Confrontation still? {E}xamining parties' policy positions In {G}reece},
	Year = {2009}}

